Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 100
       , lowerOnStart = True
       , commands = [ Run Weather "KSEA" ["-t","<tempC>C","-L","17","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run MultiCpu ["-t","Cpu: <total0>% <total1>%","-L","3","-H","50","--normal","green","--high","red","--low","white"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
		    , Run BatteryP ["CMB1", "CMB2"]
                           ["-t", "Batt: <left>%"] 10
		    , Run CpuFreq ["-t","Freq: <cpu0> <cpu1>","-L","0","-H","2","-l","lightblue","-n","white","-h","red"] 50
            , Run CoreTemp ["-t","Temp: <core0> <core1>C","-L","40","-H","60","-l","lightblue","-n","gray90","-h","red"] 50 
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %memory% * %swap% | %battery% | %multicpu% | %cpufreq% | %coretemp%            <fc=#ee9a00>%date%</fc> | %KSEA%" 
       }
       
